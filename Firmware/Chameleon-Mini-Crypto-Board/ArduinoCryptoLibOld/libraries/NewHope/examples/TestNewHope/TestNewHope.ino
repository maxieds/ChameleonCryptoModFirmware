uint8_t buffer[2048];
uint8_t random_data[64];
NewHopePrivateKey alice_private;

void testNewHopeRandom()
{
    unsigned long start;
    unsigned long elapsed;

    Serial.println("Random vector tests:");
    Serial.println();

    Serial.print("keygen ref ... ");
    start = micros();
    NewHope::keygen(buffer, alice_private);
    elapsed = micros() - start;
    Serial.print(elapsed);
    Serial.println(" us");

    Serial.print("sharedb ... ");
    start = micros();
    NewHope::sharedb(random_data, buffer, buffer);
    elapsed = micros() - start;
    Serial.print(elapsed);
    Serial.println(" us");

    Serial.print("shareda ... ");
    start = micros();
    NewHope::shareda(buffer, alice_private, buffer);
    elapsed = micros() - start;
    Serial.print(elapsed);
    Serial.println(" us");

    if (memcmp(random_data, buffer, 32) == 0)
        Serial.println("shared secrets match");
    else
        Serial.println("shared secrets do not match");
    Serial.println();

    Serial.print("keygen torref ... ");
    start = micros();
    NewHope::keygen(buffer, alice_private, NewHope::Torref);
    elapsed = micros() - start;
    Serial.print(elapsed);
    Serial.println(" us");

    Serial.print("sharedb ... ");
    start = micros();
    NewHope::sharedb(random_data, buffer, buffer, NewHope::Torref);
    elapsed = micros() - start;
    Serial.print(elapsed);
    Serial.println(" us");

    Serial.print("shareda ... ");
    start = micros();
    NewHope::shareda(buffer, alice_private, buffer);
    elapsed = micros() - start;
    Serial.print(elapsed);
    Serial.println(" us");

    if (memcmp(random_data, buffer, 32) == 0)
        Serial.println("shared secrets match");
    else
        Serial.println("shared secrets do not match");
    Serial.println();
}

void testNewHopeFixed()
{
    unsigned long start;
    unsigned long elapsed;

    Serial.println("Fixed vector tests:");
    Serial.println();

    Serial.print("keygen ref ... ");
    start = micros();
    memcpy_P(random_data, alice_random_data_ref, 64);
    NewHope::keygen(buffer, alice_private, NewHope::Ref, random_data);
    elapsed = micros() - start;
    if (memcmp_P(buffer, alice_public_ref, 1824) == 0)
        Serial.print("ok ... ");
    else
        Serial.print("fail ... ");
    Serial.print(elapsed);
    Serial.println(" us");

    Serial.print("sharedb ... ");
    start = micros();
    memcpy_P(random_data, bob_random_data_ref, 32);
    memcpy_P(buffer, alice_public_ref, 1824);
    NewHope::sharedb(random_data, buffer, buffer, NewHope::Ref, random_data);
    elapsed = micros() - start;
    bool ok = true;
    if (memcmp_P(buffer, bob_public_ref, 2048) != 0) {
        Serial.print("public fail ... ");
        ok = false;
    }
    if (memcmp_P(random_data, shared_secret_ref, 32) != 0) {
        Serial.print("shared fail ... ");
        ok = false;
    }
    if (ok)
        Serial.print("ok ... ");
    Serial.print(elapsed);
    Serial.println(" us");

    Serial.print("shareda ... ");
    start = micros();
    memcpy_P(buffer, bob_public_ref, 2048);
    NewHope::shareda(buffer, alice_private, buffer);
    elapsed = micros() - start;
    if (memcmp_P(buffer, shared_secret_ref, 32) == 0)
        Serial.print("ok ... ");
    else
        Serial.print("fail ... ");
    Serial.print(elapsed);
    Serial.println(" us");

    Serial.println();

    Serial.print("keygen torref ... ");
    start = micros();
    memcpy_P(random_data, alice_random_data_torref, 64);
    NewHope::keygen(buffer, alice_private, NewHope::Torref, random_data);
    elapsed = micros() - start;
    if (memcmp_P(buffer, alice_public_torref, 1824) == 0)
        Serial.print("ok ... ");
    else
        Serial.print("fail ... ");
    Serial.print(elapsed);
    Serial.println(" us");

    Serial.print("sharedb ... ");
    start = micros();
    memcpy_P(random_data, bob_random_data_torref, 32);
    memcpy_P(buffer, alice_public_torref, 1824);
    NewHope::sharedb(random_data, buffer, buffer, NewHope::Torref, random_data);
    elapsed = micros() - start;
    ok = true;
    if (memcmp_P(buffer, bob_public_torref, 2048) != 0) {
        Serial.print("public fail ... ");
        ok = false;
    }
    if (memcmp_P(random_data, shared_secret_torref, 32) != 0) {
        Serial.print("shared fail ... ");
        ok = false;
    }
    if (ok)
        Serial.print("ok ... ");
    Serial.print(elapsed);
    Serial.println(" us");

    Serial.print("shareda ... ");
    start = micros();
    memcpy_P(buffer, bob_public_torref, 2048);
    NewHope::shareda(buffer, alice_private, buffer);
    elapsed = micros() - start;
    if (memcmp_P(buffer, shared_secret_torref, 32) == 0)
        Serial.print("ok ... ");
    else
        Serial.print("fail ... ");
    Serial.print(elapsed);
    Serial.println(" us");
}

void setup()
{
    Serial.begin(9600);

    // Start the random number generator.  We don't initialise a noise
    // source here because we don't need one for testing purposes.
    // Real applications should of course use a proper noise source.
    RNG.begin("TestNewHope 1.0");

    // Perform the tests.
    Serial.println();
    testNewHopeRandom();
    testNewHopeFixed();
}

